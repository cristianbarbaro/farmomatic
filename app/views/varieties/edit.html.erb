<h3>Editar variedad</h3>
<hr>
<%= render 'form', variety: @variety %>

<div class="pull-right">
    <%= link_to 'Borrar', @variety, class:'btn btn-danger', method: :delete, data: { confirm: '¿Estás seguro que quieres eliminar el elemento?' }, title: 'Borrar' %>
    <%= link_to 'Ver', @variety, class: "btn btn-primary" %>
    <%= link_to 'Atrás', :back, class: "btn btn-default" %>
</div>